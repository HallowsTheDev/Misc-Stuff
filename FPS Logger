-- Scripts:

local function IMWBU_fake_script() -- Ping.PingScript 
	local script = Instance.new('LocalScript', Ping)

	local PingLabel = script.Parent.PingLabel
	
	while true do
		local UnfilteredPing = tonumber(game:GetService("Stats"):FindFirstChild("PerformanceStats").Ping:GetValue())
		local RealPing = math.floor(UnfilteredPing)
	
		PingLabel.Text = "PING: " .. RealPing
		task.wait()
	end
end
coroutine.wrap(IMWBU_fake_script)()
local function TZSYXBR_fake_script() -- FPS.FPS Script 
	local script = Instance.new('LocalScript', FPS)

	local FPS = 0
	
	local FPS_Label = script.Parent.FPSLabel
	local RS = game:GetService("RunService")
	
	RS.RenderStepped:Connect(function()
		FPS = FPS + 1
	end)
	
	while true do
		FPS_Label.Text = "FPS: " .. FPS
		FPS = 0
		task.wait(1)
	end
end
coroutine.wrap(TZSYXBR_fake_script)()
local function HOKTLD_fake_script() -- Heals.LocalScript 
