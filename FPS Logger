end
coroutine.wrap(HOKTLD_fake_script)()
local function PBPG_fake_script() -- AFK.AFK_Script 
	local script = Instance.new('LocalScript', AFK)

	local Button = script.Parent.AFK_Button
	
	local RS = game:GetService("RunService")
	local path = game:GetService("ReplicatedStorage").Events.UseBagltem
	
	local PLAYER = game:GetService("Players").LocalPlayer.Name
	local AVATAR = game:GetService("Workspace"):WaitForChild(PLAYER)
	
	local Holder = script.Parent.Parent.Parent.Holder
	local SpecHealNumber = Holder.SpecificHeal.SpecNumber
	local SpecHealLabel = Holder.SpecificHeal.SpecLabel
	local HealsNumber = Holder.Heals.HealsNumber
	
	_G.AFK_Status = false
	
	function ButtonPressed()
		if _G.AFK_Status == false then
			Button.Text = "AFK: ON"
			_G.AFK_Status = true
			_G.Toggle = false
			_G.SpeedToggle = false
		else
			_G.AFK_Status = false
			Button.Text = "AFK: OFF"
		end
	end
	
	Button.MouseButton1Down:Connect(ButtonPressed)
		
	
	RS.RenderStepped:Connect(function()
		local HUMANOID = AVATAR.Humanoid
		
		if _G.AFK_Status == true then
			if HUMANOID.Health < 100 then
				path:FireServer(_G.BaseHeal)
				HealsNumber.Text = _G.MainCount + 1
				_G.MainCount = _G.MainCount + 1
	
				if _G.BaseHeal == "Bloodfruit" then
					SpecHealNumber.Text = _G.BloodCount + 1
					SpecHealLabel.Text = "Blood: "
					_G.BloodCount = _G.BloodCount + 1
				elseif _G.BaseHeal == "Berry" then
					SpecHealLabel.Text = "Berry: "
					SpecHealNumber.Text = _G.BerryCount + 1
					_G.BerryCount = _G.BerryCount + 1
				elseif _G.BaseHeal == "Bluefruit" then
					SpecHealLabel.Text = "Blue: "
					SpecHealNumber.Text = _G.BlueCount + 1
					_G.BlueCount = _G.BlueCount + 1
				end
			end
		end
	end)
end
coroutine.wrap(PBPG_fake_script)()
local function EIWR_fake_script() -- CloseButton.CloseScript 
	local script = Instance.new('LocalScript', CloseButton)

	local UIS = game:GetService("UserInputService")
	local Holder = script.Parent.Parent.Parent.Parent.Holder
	local Button = script.Parent
	local Heals = Holder.Heals
	local close = Holder.Close
	local Speed = Holder.Speed
	local Ping = Holder.Ping
	local AFK = Holder.AFK
	local FPS = Holder.FPS
	
	UIS.InputBegan:Connect(function(INPUT)
		if INPUT.KeyCode == Enum.KeyCode.RightShift then
			AFK.Visible = true
			close.Visible = true
			FPS.Visible = true
			Heals.Visible = true
			Ping.Visible = true
			Speed.Visible = true
		end
	end)
	
	function ButtonPressed()
		AFK.Visible = false
		close.Visible = false
		FPS.Visible = false
		Heals.Visible = false
		Ping.Visible = false
		Speed.Visible = false
		
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "UI Closed",
			Text = "Press Right Shift To Open.",
		})
	end
	
	Button.MouseButton1Down:Connect(ButtonPressed)
	
	
end
coroutine.wrap(EIWR_fake_script)()
local function OCMKZ_fake_script() -- Holder.KeyHandler 
	local script = Instance.new('LocalScript', Holder)

	_G.Speed = false
	_G.AFK = false
	_G.Heals = 0
	
	_G.BaseHeal = "Bloodfruit"
	_G.BaseLabel = "Blood"
	_G.Toggle = false
	_G.SpeedToggle = false
	
	local Holder = script.Parent.Parent.Holder
	
	local UIS = game:GetService("UserInputService")
	local SpecHeal = Holder.SpecificHeal.SpecLabel
	local Heals = Holder.Heals.HealsLabel
	local Speed = Holder.Speed.SpeedLabel
	local Ping = Holder.Ping.PingLabel
	local FPS = Holder.FPS.FPSLabel
	local close = Holder.Close
	
	local SpecHealNumber = Holder.SpecificHeal.SpecNumber
	local HealsNumber = Holder.Heals.HealsNumber
	
	local temp = false
	
	_G.MainCount = 0
	_G.BlueCount = 0
	_G.BloodCount = 0
	_G.BerryCount = 0
	
	game:WaitForChild("Players").LocalPlayer:GetMouse().KeyDown:Connect(function(key)
		if key == 'q' then
			if _G.Toggle == false then
				Holder.SpecificHeal.Visible = true
				_G.Toggle = true
			else
				_G.Toggle = false
			end
		end
		if key == 'e' then
			if _G.BaseHeal == "Bloodfruit" then
				_G.BaseHeal = "Bluefruit"
				_G.BaseLabel = "Blue: "
				Holder.SpecificHeal.Visible = true
				SpecHeal.Text = "Blue: "
				SpecHealNumber = tostring(_G.BlueCount)
			elseif _G.BaseHeal == "Bluefruit" then
				_G.BaseHeal = "Berry"
				_G.BaseLabel = "Berry: "
				Holder.SpecificHeal.Visible = true
				SpecHeal.Text = "Berry: "
				SpecHealNumber = tostring(_G.BerryCount)
			elseif _G.BaseHeal == "Berry" then
				_G.BaseHeal = "Bloodfruit"
				_G.BaseLabel = "Blood: "
				Holder.SpecificHeal.Visible = true
				SpecHeal.Text = "Blood: "
				SpecHealNumber = tostring(_G.BloodCount)
			end
		end
	end)
	
	UIS.InputBegan:Connect(function(INPUT)
		if INPUT.KeyCode == Enum.KeyCode.F1 then
			if _G.SpeedToggle == false then
				_G.SpeedToggle = true
				Speed.Text = "Speed: Fast"
			elseif _G.SpeedToggle == true then
				_G.SpeedToggle = false
				Speed.Text = "Speed: Slow"
			end
		end
	end)
	
end
coroutine.wrap(OCMKZ_fake_script)()
local function JITLCJ_fake_script() -- SpecificHeal.SpecScript 
	local script = Instance.new('LocalScript', SpecificHeal)

	while true do
		task.wait(5)
		if _G.Toggle == false and _G.SpeedToggle == false then
			script.Parent.Visible = false
		end
	end
end
coroutine.wrap(JITLCJ_fake_script)()
local function JEHM_fake_script() -- Holder.HealingHandler 
	local script = Instance.new('LocalScript', Holder)

	local RS = game:GetService("RunService")
	local path = game:GetService("ReplicatedStorage").Events.UseBagltem
	
	local Holder = script.Parent.Parent.Holder
	
	local SpecHealNumber = Holder.SpecificHeal.SpecNumber
	local SpecHealLabel = Holder.SpecificHeal.SpecLabel
	local HealsNumber = Holder.Heals.HealsNumber
	
	RS.RenderStepped:Connect(function()
		if _G.Toggle == true and _G.SpeedToggle == true then
			path:FireServer(_G.BaseHeal)
			path:FireServer(_G.BaseHeal)
			HealsNumber.Text = _G.MainCount + 2
			_G.MainCount = _G.MainCount + 2
			
			if _G.BaseHeal == "Bloodfruit" then
				SpecHealLabel.Text = "Blood: "
				SpecHealNumber.Text = _G.BloodCount + 2
				_G.BloodCount = _G.BloodCount + 2
			elseif _G.BaseHeal == "Berry" then
				SpecHealLabel.Text = "Berry: "
				SpecHealNumber.Text = _G.BerryCount + 2
				_G.BerryCount = _G.BerryCount + 2
			elseif _G.BaseHeal == "Bluefruit" then
				SpecHealLabel.Text = "Blue: "
				SpecHealNumber.Text = _G.BlueCount + 2
				_G.BlueCount = _G.BlueCount + 2
			end
		end
	end)
	
	while true do
		if _G.Toggle == true and _G.SpeedToggle == false then
			path:FireServer(_G.BaseHeal)
			HealsNumber.Text = _G.MainCount + 1
			_G.MainCount = _G.MainCount + 1
	
			if _G.BaseHeal == "Bloodfruit" then
				SpecHealNumber.Text = _G.BloodCount + 1
				SpecHealLabel.Text = "Blood: "
				_G.BloodCount = _G.BloodCount + 1
			elseif _G.BaseHeal == "Berry" then
				SpecHealLabel.Text = "Berry: "
				SpecHealNumber.Text = _G.BerryCount + 1
				_G.BerryCount = _G.BerryCount + 1
			elseif _G.BaseHeal == "Bluefruit" then
				SpecHealLabel.Text = "Blue: "
				SpecHealNumber.Text = _G.BlueCount + 1
				_G.BlueCount = _G.BlueCount + 1
			end
		end
		wait()
	end
end
coroutine.wrap(JEHM_fake_script)()
local function FZUNZ_fake_script() -- BoogaHealerV3.IntroScript 
	local script = Instance.new('LocalScript', BoogaHealerV3)

	wait(1)
	
	game:GetService("StarterGui"):SetCore("SendNotification",{
		Title = "Booga Healer V3",
		Text = "Simply The Best",
	})
	
	wait()
	
	script:Destroy()
end
coroutine.wrap(FZUNZ_fake_script)()

local VU = game:GetService("VirtualUser")
game:GetService('Players').LocalPlayer.Idled:connect(function()
	VU:CaptureController()
	VU:ClickButton2(Vector2.new())
end)
